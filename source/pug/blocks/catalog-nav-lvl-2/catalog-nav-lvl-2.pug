mixin catalog-nav-lvl-2(data)
    - var className = 'catalog-nav-lvl-2'

    +el({
        class: className,
        mod: data.mod,
        mix: data.mix,
        js: data.js,
    })&attributes(attributes)
        +el({
            tag: 'ul',
            class: className + '__list',
        })
            each item, index in data.items
                +el({
                    tag: 'li',
                    class: className + '__item',
                    mix: item.mix,
                    mod: item.mod,
                })(itemprop='name')
                    +el({
                        tag: 'a',
                        class: className + '__link',
                        text: item.text
                    })(itemprop='url')
