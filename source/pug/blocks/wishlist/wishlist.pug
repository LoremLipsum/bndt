mixin wishlist(data)
    +el({
        tag: 'section',
        class: 'wishlist',
        mod: data.mod,
        mix: data.mix,
        js: data.js,
    })&attributes(attributes)
        .wishlist__wrap
            +el({
                class: 'table-goods',
                mix: 'wishlist__table'
            })
                each item in data.items
                    .table-goods__row
                        +el({
                            class: 'table-goods__cell',
                            mod: 'image',
                        })
                            +el({
                                tag: 'a',
                                class: 'table-goods__image-wrap',
                            })
                                +el({
                                    tag: 'img',
                                    class: 'table-goods__image',
                                    upload: item.image,
                                })
                        +el({
                            class: 'table-goods__cell',
                            mod: 'info',
                        })
                            .table-goods__cell-inner
                                +el({
                                    tag: 'h3',
                                    class: 'table-goods__heading',
                                    text: item.heading
                                })
                                +el({
                                    class: 'table-goods__text',
                                    mod: 'articul',
                                    text: 'артикул: 28540040'
                                })
                                +button-remove({
                                    text: 'удалить',
                                    mix: 'table-goods__button-remove',
                                    mod: 'w-text',
                                    position: 'before',
                                })
                        +el({
                            class: 'table-goods__cell',
                            mod: 'price',
                        })
                            +el({
                                class: 'table-goods__title',
                                text: 'Цена'
                            })
                            +el({
                                class: 'table-goods__value',
                                text: '2300 Р'
                            })
                        +el({
                            class: 'table-goods__cell',
                            mod: 'button',
                        })
                            +f({

                            })
                                +button({
                                    type: 'submit',
                                    text: 'Добавить в корзину',
                                    mod: 'bg-primary',
                                    mix: 'table-goods__button',
                                })
